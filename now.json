{
  "version": 2,
  "public": false,
  "name": "appli.lu",
  "alias": ["appli.lu", "www.appli.lu"],
  "regions": ["bru1"],
  "builds": [{ "src": "dist/**", "use": "@now/static" }],
  "routes": [
    { "src": "^/css/(.*)", "dest": "/dist/css/$1" },
    { "src": "^/img/(.*)", "dest": "/dist/img/$1" },
    { "src": "^/js/(.*)", "dest": "/dist/js/$1" },
    { "src": "/manifest.json", "dest": "/dist/manifest.json" },
    { "src": "/robots.txt", "dest": "/dist/robots.txt" },
    { "src": "/favicon.ico", "dest": "/dist/favicon.ico" },
    { "src": "/service-worker.js", "dest": "/dist/service-worker.js" },
    { "src": "/precache-manifest.(.*)", "dest": "/dist/precache-manifest.$1" },
    { "src": ".*", "dest": "/dist/index.html" }
  ]
}